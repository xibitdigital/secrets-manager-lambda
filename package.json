{
  "name": "aws-lambda-typescript",
  "version": "1.0.0",
  "description": "AWS Lambda with Typescript",
  "license": "GPL-3.0-only",
  "repository": "github:atombrenner/aws-lambda-typescript",
  "scripts": {
    "clean": "rimraf ./build",
    "build": "npm-run-all lint format clean && tsc",
    "start": "node src",
    "local": "ts-node src",
    "local:watch": "nodemon src -e ts,json --exec 'npm run local'",
    "lint": "eslint src",
    "format": "npx prettier --write src",
    "format:check": "npx prettier --check src",
    "test": "jest",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.329.0",
    "aws-sdk": "^2.1375.0",
    "install": "^0.13.0",
    "npm": "^9.6.6"
  },
  "devDependencies": {
    "@atombrenner/cfn-stack": "^2.0.2",
    "@aws-sdk/client-lambda": "^3.121.0",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.10.4",
    "@tsconfig/node16": "^1.0.3",
    "@types/adm-zip": "^0.5.0",
    "@types/aws-lambda": "^8.10.97",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.17.4",
    "@types/supertest": "^2.0.12",
    "aws-sdk-client-mock": "0.5.3",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.1.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.14.39",
    "husky": "^8.0.1",
    "jest": "^27.0.6",
    "prettier": "^2.8.8",
    "rome": "^12.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.7.0",
    "typescript": "4.3.5"
  }
}